<script>
  // Simple calculator logic to update table and KPIs
  function money(v){ return Number(v || 0).toFixed(2); }
  function pct(curr, fut){
    curr = Number(curr || 0); fut = Number(fut || 0);
    if(curr === 0 && fut === 0) return '0%';
    if(curr === 0) return '∞';
    return (( (fut - curr) / curr ) * 100).toFixed(1) + '%';
  }

  function setEl(id, v){ const e = document.getElementById(id); if(e) e.textContent = typeof v === 'number' ? '$' + money(v) : v; }

  function computeAll(){
    const sC = Number(document.getElementById('storage-current').value || 0);
    const sF = Number(document.getElementById('storage-future').value || 0);
    const rC = Number(document.getElementById('requests-current').value || 0);
    const rF = Number(document.getElementById('requests-future').value || 0);
    const tC = Number(document.getElementById('transfer-current').value || 0);
    const tF = Number(document.getElementById('transfer-future').value || 0);
    const kC = Number(document.getElementById('kms-current').value || 0);
    const kF = Number(document.getElementById('kms-future').value || 0);
    const lC = 0;
    const lF = Number(document.getElementById('lens-future').value || 0);

    const totalC = sC + rC + tC + kC + lC;
    const totalF = sF + rF + tF + kF + lF;
    const diffTotal = totalF - totalC;

    // update table
    setEl('tbl-storage-current', sC);
    setEl('tbl-storage-future', sF);
    setEl('tbl-storage-diff', sF - sC);
    document.getElementById('tbl-storage-pct').textContent = pct(sC, sF);

    setEl('tbl-requests-current', rC);
    setEl('tbl-requests-future', rF);
    setEl('tbl-requests-diff', rF - rC);
    document.getElementById('tbl-requests-pct').textContent = pct(rC, rF);

    setEl('tbl-transfer-current', tC);
    setEl('tbl-transfer-future', tF);
    setEl('tbl-transfer-diff', tF - tC);
    document.getElementById('tbl-transfer-pct').textContent = pct(tC, tF);

    setEl('tbl-kms-current', kC);
    setEl('tbl-kms-future', kF);
    setEl('tbl-kms-diff', kF - kC);
    document.getElementById('tbl-kms-pct').textContent = pct(kC, kF);

    setEl('tbl-lens-current', lC);
    setEl('tbl-lens-future', lF);
    setEl('tbl-lens-diff', lF - lC);
    document.getElementById('tbl-lens-pct').textContent = pct(lC, lF);

    setEl('tbl-total-current', totalC);
    setEl('tbl-total-future', totalF);
    setEl('tbl-total-diff', diffTotal);
    document.getElementById('tbl-total-pct').textContent = pct(totalC, totalF);

    // KPIs
    document.getElementById('total-current').textContent = '$' + money(totalC);
    document.getElementById('total-future').textContent = '$' + money(totalF);

    // Exec summary quick-fill
    document.getElementById('exec-cur').textContent = '$' + money(totalC);
    document.getElementById('exec-fut').textContent = '$' + money(totalF);
    document.getElementById('exec-inc').textContent = '$' + money(diffTotal);
    document.getElementById('exec-inc-pct').textContent = pct(totalC, totalF);

    // colorize percent
    const pctEl = document.getElementById('tbl-total-pct');
    if(pctEl){
      const txt = pctEl.textContent;
      if(txt === '∞' || txt.includes('-')) pctEl.className = 'percent-down'; else pctEl.className = 'percent-up';
    }
  }

  // initial compute
  computeAll();
</script>
